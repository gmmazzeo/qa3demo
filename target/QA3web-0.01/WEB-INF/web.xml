<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	 version="3.0">
    <servlet>
        <servlet-name>TagServlet</servlet-name>
        <servlet-class>edu.ucla.cs.scai.qa3web.servlet.TagServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FillOutServlet</servlet-name>
        <servlet-class>edu.ucla.cs.scai.qa3web.servlet.FillOutServlet</servlet-class>
    </servlet>    
    <servlet>
        <servlet-name>QueryServlet</servlet-name>
        <servlet-class>edu.ucla.cs.scai.qa3web.servlet.QueryServlet</servlet-class>
    </servlet>    
    <servlet>
        <servlet-name>TemplateServlet</servlet-name>
        <servlet-class>edu.ucla.cs.scai.qa3web.servlet.TemplateServlet</servlet-class>
    </servlet>  
    <servlet-mapping>
        <servlet-name>TagServlet</servlet-name>
        <url-pattern>/tag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FillOutServlet</servlet-name>
        <url-pattern>/fillout</url-pattern>
    </servlet-mapping>    
    <servlet-mapping>
        <servlet-name>TemplateServlet</servlet-name>
        <url-pattern>/template</url-pattern>
    </servlet-mapping>    
    <servlet-mapping>
        <servlet-name>QueryServlet</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>      
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <listener>
        <listener-class>
            edu.ucla.cs.scai.qa3web.MyAppServletContextListener
        </listener-class>
    </listener>
</web-app>
